    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.10",
    "namor": "^2.0.2",
    "react-bootstrap": "^1.0.1",
